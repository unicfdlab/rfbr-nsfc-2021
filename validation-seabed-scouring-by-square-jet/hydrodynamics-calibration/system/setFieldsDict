FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (0 0 0)
);

regions
(
    boxToCell
    {
        box (0 0.05 -0.05) (6 0.15 0.05);
        fieldValues
        (
            volVectorFieldValue U (1.21 0 0)
        );
    }
);


