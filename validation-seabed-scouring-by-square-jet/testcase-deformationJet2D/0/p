FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "1e-05";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    "(inlet|top|down|left|wallSop)"
    {
        type            zeroGradient;
    }

    "(right)"
    {
        type            fixedValue;
        value           uniform 0;
    }
    "(side1|side2)"
    {
        type            empty;
    }

}


